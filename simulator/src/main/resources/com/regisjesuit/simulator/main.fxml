<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.regisjesuit.simulator.controllers.Controller">
    <Button onMouseClicked="#runButtonClick">Run</Button>
    <padding>
        <Insets bottom="20.0" top="10.0"/>
    </padding>
    <StackPane style="-fx-border-color: black;" VBox.vgrow="NEVER">
        <Canvas fx:id="canvas" height="319.0" onMouseClicked="#onCanvasClick"
          onMouseMoved="#onMouseMove" width="649.0"/>
    </StackPane>
</VBox>
